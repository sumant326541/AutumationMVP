{
  "name": "protractor-multiple-cucumber-html-reporter-plugin",
  "version": "1.8.1",
  "description": "A protractor plugin to use multiple-cucumber-html-reporter with CucumberJs 1, 2 or 3",
  "main": "index.js",
  "scripts": {
    "lint": "node_modules/.bin/eslint *.js test/**/*.js",
    "pretest": "webdriver-manager update",
    "release": "np",
    "test": "npm run lint && node node_modules/.bin/jasmine ./test/test.spec.js",
    "test.single": "./node_modules/.bin/protractor ./test/cucumber/config/full-options.conf.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wswebcreation/protractor-multiple-cucumber-html-reporter-plugin.git"
  },
  "keywords": [
    "protractor",
    "cucumber",
    "html",
    "test report",
    "multiple-cucumber-html-reporter",
    "html report",
    "json to html"
  ],
  "author": "wswebcreation",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wswebcreation/protractor-multiple-cucumber-html-reporter-plugin/issues"
  },
  "homepage": "https://github.com/wswebcreation/protractor-multiple-cucumber-html-reporter-plugin#readme",
  "devDependencies": {
    "cucumber": "^5.1.0",
    "eslint": "^4.11.0",
    "glob": "^7.1.2",
    "np": "^5.0.2",
    "protractor": "^5.4.1",
    "protractor-cucumber-framework": "^6.1.1"
  },
  "peerDependencies": {
    "cucumber": ">= 1.3.0 || >= 2.0.0 || >= 3.0.0",
    "protractor-cucumber-framework": ">= 3.1.2",
    "protractor": ">= 4.0.0"
  },
  "dependencies": {
    "fs-extra": "^3.0.1",
    "multiple-cucumber-html-reporter": "^1.11.7"
  }
}
